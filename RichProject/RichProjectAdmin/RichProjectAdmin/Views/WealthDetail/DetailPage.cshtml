@using Microsoft.AspNetCore.Mvc.TagHelpers

@{
    ViewData["Title"] = "DetailPage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<table class="layui-hide" id="demo" lay-filter="test"></table>
@await Html.PartialAsync("Partial/_WealthDetailPage")

<script type="text/html" id="barDemo">
    <a class="layui-btn layui-btn-primary layui-btn-xs" lay-event="detail">查看</a>
    <a class="layui-btn layui-btn-xs" lay-event="edit">编辑</a>
    <a class="layui-btn layui-btn-danger layui-btn-xs" lay-event="del">删除</a>
</script>
<script>
    var Url = {
        GetWealthDetail: '@Url.Action("GetWealthDetailFetch", "WealthDetail")',
        DelWealthDetail: '@Url.Action("DeleteWealthDetailById", "WealthDetail")',
        UpdateWealthDetail: '@Url.Action("UpdateWealthDetailById", "WealthDetail")',
        AddWealthDetail:'@Url.Action("AddWealthDetail", "WealthDetail")'
    }
</script>
<script src="~/js/WealthDetail/DetailPage.js"></script>
